<!DOCTYPE html>
<html>
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<h2>Students</h2>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Pass</th>
  </tr>

  <% for(var i =0; i < customer.length ; i++){ %>
  <tr>
    <td><%= customer[i].id %></td>
    <td><%= customer[i].name %></td>
    <td><%= customer[i].pass%></td>
  </tr>
 <% } %>
</table>

</body>
</html>
